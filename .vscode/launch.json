{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug with GDB",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/A星寻路算法.out",
          "args": [],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "miDebuggerPath": "/usr/bin/gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
          "preLaunchTask": "build",
          "miDebuggerArgs": "",
          "logging": {
              "trace": false,
              "traceResponse": false,
              "engineLogging": false,
              "programOutput": false,
              "exceptions": false
          }
      }
  ]
}
